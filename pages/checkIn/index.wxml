		<!-- <van-icon name="arrow-left" /> -->
		<text>入住办理</text>
	</view>
</van-nav-bar>
<view class="list_box" bindtap="toHotel" data-id="11">
  <view>
    <image class="scenic_img" src="{{iconUrl}}/lhfz.png"></image>
    <view class="details_box">
      <view class="bottom_shadow"></view>
      <view class="details_box_left">莲花山庄</view>
      <view class="details_box_right">
        <image class="qrcode_img" src="{{iconUrl}}/lhfzewm.png"></image>
        <view class="qrcode_title">点击下载二维码</view>
        <view class="qrcode_title">在线办理酒店入住</view>
      </view>
    </view>
  </view>
</view>
